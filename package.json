{
  "name": "@etchteam/mobius",
  "version": "1.0.0",
  "description": "WRAP's design system",
  "scripts": {
    "build": "webpack --mode production",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint \"./**/*.{htm,html,css,sss,less,scss,sass}\"",
    "lint:css:fix": "npm run lint:css -- --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:a11y:from-build": "node ./scripts/storybook-a11y-tests.js",
    "commitlint": "commitlint",
    "pre-commit": "lint-staged",
    "prepare": "node ./scripts/install-husky.js && node ./scripts/export-components.js",
    "release": "semantic-release"
  },
  "private": true,
  "engines": {
    "npm": ">=8.5.5",
    "node": ">=16.15.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{css,sss,less,scss,sass}": "stylelint --fix --allow-empty-input"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@shopify/storybook-a11y-test": "1.1.0",
    "@storybook/addon-a11y": "6.5.12",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.17",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "8.25.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.5.1",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "glob": "8.0.3",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "postcss": "8.4.18",
    "prettier": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.54.4",
    "sass-loader": "^13.0.2",
    "semantic-release": "19.0.5",
    "style-loader": "^3.3.1",
    "stylelint": "^14.10.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "ts-loader": "^9.3.1",
    "tsconfig-paths": "4.1.0",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "typescript": "4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@svgr/webpack": "^6.3.1",
    "classnames": "^2.3.1",
    "mono-icons": "^1.3.1"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ]
}
